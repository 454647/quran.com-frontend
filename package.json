{
  "name": "quran",
  "version": "1.0.0",
  "private": false,
  "repository": "https://github.com/quran/quran.com-frontend",
  "scripts": {
    "analyze:client": "babel-node --extensions '.ts,.tsx' internal/scripts/analyze -- --client",
    "analyze:server": "babel-node --extensions '.ts,.tsx' internal/scripts/analyze -- --server",
    "build-storybook": "build-storybook",
    "build:dev": "babel-node --extensions '.ts,.tsx' internal/scripts/build",
    "build": "babel-node --extensions '.ts,.tsx' internal/scripts/build -- --optimize",
    "bundlesize": "bundlesize",
    "clean": "cross-env babel-node --extensions '.ts,.tsx' internal/scripts/clean",
    "dev": "cross-env DEPLOYMENT=development babel-node --extensions '.ts,.tsx' internal/development",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint ./src",
    "precommit": "lint-staged",
    "preinstall": "node internal/scripts/preinstall",
    "prettier": "prettier --single-quote --write --parser=typescript",
    "prettier:scss": "prettier --single-quote --write --parser=scss",
    "start": "cross-env NODE_ENV=production node build/server",
    "test:stylelint": "stylelint './src/**/*.scss' --config ./.stylelintrc",
    "test": "npm run lint; npm run jest",
    "type": "tsc --build tsconfig.json",
    "validate": "npm ls",
    "storybook": "start-storybook -p 6006"
  },
  "bundlesize": [
    {
      "path": "./build/client/index-*.js",
      "maxSize": "210 kB"
    },
    {
      "path": "./build/client/Chapter-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./build/client/index-*.css",
      "maxSize": "20 kB"
    }
  ],
  "pre-commit": [
    "test:stylelint",
    "precommit"
  ],
  "lint-staged": {
    "*.tsx": [
      "npm run prettier",
      "npm run lint:fix",
      "git add"
    ],
    "*.ts": [
      "npm run prettier",
      "npm run lint:fix",
      "git add"
    ],
    "*.scss": [
      "npm run prettier:scss",
      "git add"
    ]
  },
